{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "modules": true,
            "arrowFunctions": true,
            "blockBindings": true,
            "destructuring": true,
            "classes": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true,
        "commonjs": true
    },
    "extends": "eslint:recommended"
}